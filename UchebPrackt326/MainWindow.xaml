<Window x:Class="UchebPrackt326.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UchebPrackt326"
        mc:Ignorable="d"
        Title="школы иностранных языков" Height="650" Width="1000" Icon="Resourses/school_logo.ico" ResizeMode="CanMinimize">
    <Grid>
        <Grid >
            <Grid.RowDefinitions>
                <RowDefinition Height="35*"/>
                <RowDefinition Height="190*"/>
            </Grid.RowDefinitions>
            <Grid Background="#FFE7FABF">
                <Image Source="Resourses/school_logo.ico" MouseDown="Image_MouseDown" HorizontalAlignment="Left" Width="85" Margin="10 6 0 0" Grid.Row="0"></Image>
                <Border HorizontalAlignment="Center" Width="400" Height="70"  CornerRadius="17" Background="#FF04A0FF">
                   
                </Border>
            </Grid>
            
            <Frame x:Name="MainFrame" NavigationUIVisibility="Hidden" Grid.Row="1"></Frame>
        </Grid>
    </Grid>
</Window>
